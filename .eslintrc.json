{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "off",
        "arrow-body-style": ["error", "as-needed"],
        "prefer-default-export": 0,
        "no-empty-function": 0,
        "import/no-unresolved":0,
        "import/extensions": 0,
        "import/prefer-default-export": 0,
        "no-useless-constructor": 0,
        "no-use-before-define":0,
        "@typescript-eslint/naming-convention": [
          "error",
          {
            "selector": "interface",
            "format": ["PascalCase"],
            "custom": {
              "regex": "^I[A-Z]",
              "match": true
            }
          }
        ]
    }
}
